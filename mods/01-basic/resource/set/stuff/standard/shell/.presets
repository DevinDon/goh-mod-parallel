; args mass fill
; calibre <= 50mm
; Shell mass < 5 kg
(define "set-explosion-level-1"
  {damage blastwave
    {energy (+ (/ %mass 50) (* 1 (+ %fill 0.2)))}
    {area (* 0.5 (+ (/ %mass 10) (* 5 (+ %fill 0.25))))    (+ (/ %mass 10) (* 5 (+ %fill 0.25)))}
    {ground_interaction_radius 0.5}
  }
  {damage add blastwave
    {energy (+ 1.1 (* 0.75 %fill))}
    {area (+ (* %mass 0.25) (+ 1 (* 5 %fill)))       (+ (* %mass 0.5) (+ 4.5 (* 5 %fill))) }
    {ground_interaction_radius 0}     ; value is multiplied by 2nd value of area to deterimine is blast is close enough to ground to trigger terrain fx
  }
)

; args mass fill
; calibre <= 90mm
