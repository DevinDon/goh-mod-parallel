    {area (+ (* %mass 0.25) (+ 2 (* 3 %fill)))      (+ (* %mass 0.5) (+ 3.5 (* 3 %fill)))}
    {ground_interaction_radius 0}     ; value is multiplied by 2nd value of area to deterimine is blast is close enough to ground to trigger terrain fx
  }
)

