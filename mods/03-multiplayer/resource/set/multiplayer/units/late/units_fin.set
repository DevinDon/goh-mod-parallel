{"doctrine_squad_command_late(fin)"
  ("squad_with4types_doctrine" side(fin) period(late) c1(officer:1) c2(squadleader:1) c3(antitank_rifle:4) c4(antitank_asst:4))
  ("doctrine_t1" cool(240) d(late_fin_allround) cost(5))
