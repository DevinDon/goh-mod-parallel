{game_entity

  (include "/properties/standard/vehicle/armored-car-config.ext")
  (include "/properties/selection/vehicle.inc" scale(1.0))

  {Extension "/entity/-vehicle/germany/btr/sdkfz251d_1/sdkfz251d_1.mdl"}
  (include   "/entity/-vehicle/germany/btr/sdkfz251d_1/sdkfz251d_1.ext")
  (include   "/entity/-vehicle/germany/btr/sdkfz251d_1/crew-and-passenger.ext")

  {Props "tank" "halftrack" "wheeled" "multi_board" "explosion_throw_crew" "vision_lev02" "tractor" "tractor_heavy" "detect_tank_mg" "open_death_fx_low"}

  {PatherId "sdkfz251"}
  {Targetclass "armoredcar"}
  {TargetSelector "tank_bullet"}

  {Extender "detail"
    {random 0}
    {detail1}
    {detail2}
    {detail3}
    {detail4}
    {detail5}
    {detail6}
    {detail7}
    {detail8}
    {detail9}
    {detail10}
    {detail11}
    {detail12}
    {detail13}
    {detail14}
    {detail15}
  }

  {Able "emit_passengers_on_enemy"}

  {Extender "enumerator"
    {digit_folder "np"}
  }

  {Extender "inventory"
    {Weapon "mgun" {mask "mountable"}}
    {Box
      {Size 15 8}
      {Weight 1000}
      {Item "repair_kit" 10}

      {Item "ammo inf-10mm-chain ap" 3250}
      {Item "weapon ger-mg42" filled {user "mgun"}}
    }
  }

  {Weaponry
    {Place "mgun"
      {Type "main"}
      {Gunner "gunner2"}
      {Charger "gunner2"}
      {LinkBone "mgun"}
      {LinkAnimation "off"}
      {basic}
      {ai_in_manual}
      {AccuracyByMotion
        {MaxLinearSpeed   8.00} ; km/h
        {ByLinearSpeed    0.40} ; km/h
        {MaxAngularSpeed 30.00} ; deg/s
        {ByAngularSpeed   0.50} ; deg/s
        {DisorderTime     0.60} ; s
        {OrderTime        1.00} ; s
      }
    }
    {Place "mgun2"
      {Type "rear"}
      {Weapon "weapon inf-infantry-general-purpose-machine-gun" filling "ammo inf-10mm-chain ap" 240}
      {Gunner "gunner4"}
      {Charger "gunner4"}
      {LinkBone "mgun2"}
      {LinkAnimation "off"}
      {basic}
      {ai_in_manual}
      {AccuracyByMotion
        {MaxLinearSpeed   8.00} ; km/h
        {ByLinearSpeed    0.40} ; km/h
        {MaxAngularSpeed 30.00} ; deg/s
        {ByAngularSpeed   0.50} ; deg/s
        {DisorderTime     0.60} ; s
        {OrderTime        1.00} ; s
      }
    }
  }

  {Mass 7810}
  {Chassis "wheel"
    ("set-tank-trace" fx("tracks_sm2") step(0.2) length(0.49) boneleft("FXtraceL3") boneright("FXtraceR3"))
    ("set-car-trace" fx("tracewheel03a") step(0.09) wheelleft("wlf") wheelright("wrf") boneleft("FXtraceL1") boneright("FXtraceR1"))
    ("set-maneuverability-of-armored-car" speed(53) power(100) mass(7810) performance(1.52))
    ("set-fuel-bag" volume(160) power(100) mass(7810))
  }

  {Bone "mgun_a_rot1"
    {speed2 60}
  }
  {Bone "mgun_a_rot2"
    {speed2 60}
  }

  {Bone "mgun_a2_rot1"
    {speed2 60}
  }
  {Bone "mgun_a2_rot2"
    {speed2 60}
  }

}
