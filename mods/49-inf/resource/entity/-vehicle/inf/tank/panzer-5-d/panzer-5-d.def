{game_entity

  ; 载具编号 sdkfz-171
  ; 载具名称 五号坦克 A 型
  ; 载具类别 tank
  ; 载具阵营 ascension
  ; 载具国家 inf

  (include "/properties/standard/vehicle/template/medium-tank.ext")
  (include "/properties/selection/vehicle.inc" scale(1.2))

  {Extension "/entity/-vehicle/germany/tank_medium/panzer5d/panzer5d.mdl"}
  (include   "/entity/-vehicle/germany/tank_medium/panzer5d/panzer5d.ext")
  (include   "/entity/-vehicle/germany/tank_medium/panzer5d/crew-and-passenger.ext")

  {PatherID "panzer5"}
  {Collider "tank.heavy"}
  {TargetClass "tank_heavy"}
  {TargetSelector "tank_heavy"}
  {Props "medium vision_lev04 muzzle_85mm_93mm_sides shield smoke nwg detect_tank_heavy"}
  {able "emit_passengers_on_enemy"}

  {Extender "detail"
    {random 0}
    {detail5}
    {detail2}
    {detail3}
    {detail4}
    {detail1}
    {detail6}
    {detail7}
    {detail8}
    {detail9}
    {detail10}
    {detail11}
    {detail13}
    {detail12}
    {detail14}
  }

  ; 无额外 Enumerator

  {Extender "shield" {on}}

  {Extender "smokescreen"
    {cooldown 5}
    {itemTags "grenade nbks"}
  }

  {Extender "inventory"
    ; 无可挂载武器
    {Box
      {Size 12 10}
      {Weight 1000}
      {Item "repair_kit" 10}
      {Item "ammo inf-10mm-chain ap" 4500}
      {Item "nbks grenade" 6}
      {Item "ammo ger-75l70 apcbche" 39}
      {Item "ammo ger-75l70 apcr" 5}
      {Item "ammo ger-75l70 he" 29}
    }
  }

  {Weaponry
    ; 无恢复 IK 时间
    {Place gun
      ; 无武器配置
      {RestoreIKAfterAim}
      ; 无需旋转车体
      {Weapon "weapon ger-75mm-kwk42-l70" filling "ammo ger-75l70 apcbche" 1}
      ; 无开火点
      {Gunner "gunner"}
      {Charger "charger"}
      ; 无链接骨骼
      ; 无链接动画
      {basic}
      {ai_in_manual}
      {AccuracyByMotion
        {MaxLinearSpeed 7}
        {ByLinearSpeed 0.25}
        {MaxAngularSpeed 3}
        {ByAngularSpeed 0.35}
        {DisorderTime 0.25}
        {OrderTime 2}
      }
    }
    {Place mgun2
      {Type "coaxial"}
      {RestoreIKAfterAim}
      ; 无需旋转车体
      {Weapon "weapon inf-infantry-general-purpose-machine-gun" filling "ammo inf-10mm-chain ap" 240}
      ; 无开火点
      {Gunner "gunner"}
      {Charger "charger"}
      ; 无链接骨骼
      ; 无链接动画
      {basic}
      {ai_in_manual}
      {AccuracyByMotion
        {MaxLinearSpeed 8}
        {ByLinearSpeed 0.4}
        {MaxAngularSpeed 30}
        {ByAngularSpeed 0.5}
        {DisorderTime 0.6}
        {OrderTime 0.75}
      }
    }
    {Place mgun1
      {Type "hull"}
      {RestoreIKAfterAim}
      ; 无需旋转车体
      {Weapon "weapon inf-infantry-general-purpose-machine-gun" filling "ammo inf-10mm-chain ap" 240}
      ; 无开火点
      {Gunner "gunner3"}
      {Charger "gunner3"}
      ; 无链接骨骼
      ; 无链接动画
      {basic}
      {ai_in_manual}
      {AccuracyByMotion
        {MaxLinearSpeed 8}
        {ByLinearSpeed 0.4}
        {MaxAngularSpeed 30}
        {ByAngularSpeed 0.5}
        {DisorderTime 0.6}
        {OrderTime 0.75}
      }
    }
  }

  {Bone "gun_rot"
    {limits -8 20}
    {speed2 4}
  }
  {Bone "turret"
    ; 无角度限制
    {speed2 6}
  }

  {Mass 45300}
  {Chassis "track"
    {Track "left"
      {Tracer
        {View "tracks_big"}
        {Bone "FXtraceL1"}
        {Step 0.47}
      }
      {TextureScroller
        {Bone "TrackL"}
        {Length 0.94}
      }
    }
    {Track "right"
      {Tracer
        {View "tracks_big"}
        {Bone "FXtraceR1"}
        {Step 0.47}
      }
      {TextureScroller
        {Bone "TrackR"}
        {Length 0.94}
      }
    }
    {Locomotion
      {Slow              10}
      {Normal            55}
      {Fast              55}
      {MaxSpeed          55}
      {MaxSpeedAtMaxTurn 38.77}
      {TurnTime          0.26}
      {TurnStart         0.81}
      {TurnStop          0.81}
      {TurnRadius        8.46}
      {StartTime         7.74}
      {StopTime          3.68}
      {BrakeTime         1.84}
      {Gears             0.5 1.0}
      {RearGears         0.07}
      {GearSwitchDelay   0.21}
      {SplineRadius      0.45}
      {MaxSlope          75}
      {FallSlope         75}
    }
    {FuelBag
      {Volume            700}
      {Rate              28.27}
      {Remain            700}
      {Fuel              "fuel"}
    }
  }
  ; 在射击时可以移动

}
