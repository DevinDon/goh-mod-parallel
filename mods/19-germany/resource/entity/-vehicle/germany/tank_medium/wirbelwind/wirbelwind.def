{game_entity

  (include "/properties/standard/vehicle/tank-config.ext")
  (include "/properties/selection/vehicle.inc" scale(1.1))

  {Extension "wirbelwind.mdl"}
  (include   "wirbelwind.ext")
  (include   "crew-and-seat.ext")

  {Props "medium" "vision_lev04" "shell_eject" "open_top" "shield" "flak"}

  {PatherId "panzer3"}
  {Targetclass "tank_medium"}
  {TargetSelector "tank_zenite"}

  {Extender "detail"
    {random 0}
    {detail5}
    {detail6}
    {detail7}
    {detail8}
    {detail9}
    {detail10}
    {detail11}
    {detail4}
    {detail3}
    {detail2}
    {detail1}
  }

  {Extender "enumerator"
    {digit_folder "ger"}
  }

  {Sensor
    {Visor "around"
      {Vision "standard-vehicle-vision-around"}
    }
  }

  {Extender "shield" {on}}

  {Extender "inventory"
    {Box
      {Size 10 10}
      {Weight 1000}
      {Item "repair_kit" 10}

      {Item "ammo ger-mgun api" 1200}

      {Item "ammo ger-20l55 ap" 200}
      {Item "ammo ger-20l55 he" 200}
    }
  }

  {Weaponry
    {RestoreIKTime 2}
    {Place "gun"
      {RestoreIKAfterAim}
      {Weapon "weapon ger-20mm-flak38-l112-x4" filling "ammo ger-20l55 he" 80}
      {Gunner "gunner"}
      {Charger "charger"}
      {AccuracyByMotion
        {MaxLinearSpeed  7.00} ; km/h
        {ByLinearSpeed   0.25} ; km/h
        {MaxAngularSpeed 3.00} ; deg/s
        {ByAngularSpeed  0.35} ; deg/s
        {DisorderTime    0.15} ; s
        {OrderTime       1.25} ; s
      }
    }
    {Place "mgun1"
      {Type "hull"}
      {Weapon "weapon ger-mg42" filled}
      {Foresight "foresight4"}
      {Gunner "gunner3"}
      {Charger "gunner3"}
      {basic}
      {ai_in_manual}
      {AccuracyByMotion
        {MaxLinearSpeed   8.00} ; km/h
        {ByLinearSpeed    0.40} ; km/h
        {MaxAngularSpeed 30.00} ; deg/s
        {ByAngularSpeed   0.50} ; deg/s
        {DisorderTime     0.60} ; s
        {OrderTime        1.00} ; s
      }
    }
  }

  {mass 22000}
  {Chassis "track"
    ("set-tank-trace" fx("tracks_mid") step(0.52) length(0.45) boneleft("FXtraceL1") boneright("FXtraceR1"))
    ("set-maneuverability-of-tank" speed(38) power(265) mass(22000) performance(1.35))
    ("set-fuel-bag" volume(470) power(265) mass(22000))
  }

  {Bone "gun_rot"
    {limits -10 88}
    {speed2 50}
  }
  {Bone "turret"
    {speed2 40}
  }

}
