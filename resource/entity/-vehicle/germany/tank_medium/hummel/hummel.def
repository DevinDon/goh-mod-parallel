{game_entity

  (include "/properties/standard/vehicle/tank-assaultgun-config.ext")
  (include "/properties/selection/vehicle_elongated.inc" scale(1.15))

  {Extension "hummel.mdl"}
  (include   "hummel.ext")
  (include   "crew-and-seat.ext")

  {Props "spg" "medium" "-turret" "multi_board" "vision_lev04" "muzzle_150mm_155mm" "shell_eject" "open_top" "detect_artillery" "artillery_minimap_heavy"}

  {PatherId "nashorn"}
  {Targetclass "tank_medium"}
  {TargetSelector "cannon_howitzer"}

  {Extender "detail"
    {random 0}
    {detail12}
    {detail1}
    {detail4}
    {detail5}
    {detail6}
    {detail9}
    {detail10}
    {detail11}
    {detail2}
    {detail3}
    {detail7}
    {detail8}
  }

  {Extender "enumerator"
    {digit_folder "ger"}
  }

  {Sensor
    {Visor "around"
      {Vision "standard-vision-around"}
    }
  }

  {Extender "shield" {on}}

  {Extender "inventory"
    {Box
      {Size 10 8}
      {Weight 1000}
      {Item "repair_kit" 10}

      {Item "ammo ger-150l30 he" 15}
    }
  }

  {Weaponry
    {RestoreIKTime 5}
    {Place "gun"
      {RestoreIKAfterAim}
      {Weapon "weapon ger-150mm-sfh18-l30" filling "ammo ger-150l30 he" 1}
      {Gunner "gunner gunner2"}
      {Charger "charger gunner3" minWorkers 1}
      {AccuracyByMotion
        {MaxLinearSpeed  7.00} ; km/h
        {ByLinearSpeed   0.25} ; km/h
        {MaxAngularSpeed 3.00} ; deg/s
        {ByAngularSpeed  0.35} ; deg/s
        {DisorderTime    1.25} ; s
        {OrderTime       7.35} ; s
      }
    }
  }

  {Mass 23000}
  {Chassis "track"
    ("set-tank-trace" fx("tracks_mid") step(0.2) length(-0.52) boneleft("FXtraceL1") boneright("FXtraceR1"))
    ("set-maneuverability-of-tank" speed(42) power(265) mass(23000) performance(1.31))
    ("set-fuel-bag" volume(600) power(265))
  }

  {Bone "gun_rot"
    {limits -3 42}
    {speed2 8}
  }
  {Bone "turret"
    {limits -15 15}
    {speed2 8}
  }

}
