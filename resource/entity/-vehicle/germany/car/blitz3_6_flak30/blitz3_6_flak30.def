      {Size 9 9}
      {Weight 2000}
      {Item "repair_kit" 20}

      {Item "ammo ger-20l55 ap" 100}
      {Item "ammo ger-20l55 he" 380}
    }
  }

  {Weaponry
    {RestoreIKTime 10}
    {Place "gun"
      {Weapon "weapon ger-20mm-flak30-l65" filling "ammo ger-20l55 he" 20}
