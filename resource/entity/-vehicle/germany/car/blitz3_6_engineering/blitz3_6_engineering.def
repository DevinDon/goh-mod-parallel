  {Extender "shield" {on}}

  {Extender "inventory"
    {Box
      {Size 15 15}
      {Weight 2000}
      {Item "repair_kit" 20}

      {Item "tmi_35" "mine" 60}
      {Item "mine_detector" 2}
      {Item "shovel_ger" 5}
      {Item "pickaxe" 5}
      {Item "geballteladung_3kg" 5}
      {Item "s" "mine" 120}
      {Item "wirecutters" 2}
    }
  }

  {mass 3600}
  {Chassis "wheel"
    ("set-car-trace" fx("tracewheel03")  step(0.08) wheelleft("wlf") wheelright("wrf") boneleft("FXtraceL1") boneright("FXtraceR1"))
    ("set-car-trace" fx("tracewheel03d") step(0.08) wheelleft("wlr") wheelright("wrr") boneleft("FXtraceL2") boneright("FXtraceR2"))
    ("set-maneuverability-of-armored-car" speed(80) power(68) mass(3200) performance(1.45))
    ("set-fuel-bag" volume(80) power(68))
  }

}
