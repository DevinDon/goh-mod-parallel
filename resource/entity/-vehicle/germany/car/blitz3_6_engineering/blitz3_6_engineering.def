{game_entity

  (include "/properties/standard/vehicle/car-config.ext")
  (include "/properties/selection/vehicle_elongated.inc" scale(1.15))
  (include "/properties/resupply.inc")

  {Extension "blitz3_6_engineering.mdl"}
  (include   "blitz3_6_engineering.ext")
  (include   "crew-and-seat.ext")




  {Props  "truck" "vision_lev02" "tractor" "tractor_heavy"}

  {PatherId "blitz36"}
  {Collider "truck"}

  {Extender "detail"
    {random 0}
    {detail1}
    {detail2}
    {detail9}
  }

  {Able "emit_on_attack"}

